
#include <iostream>
using namespace std;
int main() 
{
    int n = 100;
    char* sss = new char[n];
    cin.get(sss, 30);
    int k = strlen(sss);
    int r = 0;
    for (int i = 0; i < k; i++) {
        if (sss[i] != sss[k - 1 - i]) { 
            cout << "No";
            r = 1;
            break;
        }
        
    }
    if (r == 0) cout << "Yes";
    delete sss;
}


